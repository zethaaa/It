<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="modelo.Historial" table="historial">
        <id name="id" type="long">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="idCita" type="long">
            <column name="id_cita" not-null="true"/>
        </property>
        <property name="diagnostico" type="string">
            <column name="diagnostico" length="255"/>
        </property>
        <property name="tratamiento" type="string">
            <column name="tratamiento" length="255"/>
        </property>
    </class>
</hibernate-mapping>

